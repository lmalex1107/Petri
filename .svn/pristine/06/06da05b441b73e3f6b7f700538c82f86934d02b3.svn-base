package view;

import java.awt.BorderLayout;
import java.util.ArrayList;

import javax.swing.JPanel;

import model.Arc;
import model.Knot;

public class GraphPanel extends JPanel{
	public GraphPanel(ArrayList<Knot> knotlist, ArrayList<Arc> arclist) {
		PetriGraph petrigraph = new PetriGraph(knotlist, arclist);
		PetriPanel petripanel = new PetriPanel(petrigraph);
		this.add(petripanel, BorderLayout.WEST);
		
	}
}
